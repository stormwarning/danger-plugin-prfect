{
  "name": "danger-plugin-prfect",
  "version": "0.0.0-development",
  "description": "PRs",
  "keywords": [
    "danger",
    "danger-plugin"
  ],
  "homepage": "https://github.com/stormwarning/danger-plugin-prfect",
  "repository": "stormwarning/danger-plugin-prfect",
  "license": "ISC",
  "author": "Jeff",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "npm run setup && esbuild src/index.ts --bundle --platform=node --target=node16 --outfile=dist/index.js",
    "predocs": "rm -rf docs/",
    "docs": "esdoc -c .esdoc.json",
    "format": "prettier --write",
    "lint": "eslint --fix",
    "prepublish": "npm run build",
    "setup": "zazen configure",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": "eslint --fix"
  },
  "devDependencies": {
    "@zazen/cli": "0.3.1",
    "danger": "*",
    "esbuild": "0.14.21",
    "eslint": "8.8.0",
    "husky": "^7.0.0",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "prettier-plugin-packagejson": "2.2.15",
    "typescript": "4.5.5"
  },
  "optionalDependencies": {
    "esdoc": "^0.5.2"
  },
  "engines": {
    "node": ">=16"
  }
}
